package com.example.examseatingarrangementfinal;

import javafx.application.Application;
import javafx.fxml.FXMLLoader;
import javafx.scene.Parent;
import javafx.scene.Scene;
import javafx.stage.Stage;
import java.io.IOException;

public class ExamSeatingArrangementApp extends Application {

    @Override
    public void start(Stage primaryStage) throws IOException {
        FXMLLoader loader = new FXMLLoader(getClass().getResource("/com/example/examseatingarrangementfinal/Welcome.fxml"));
        Scene scene = new Scene(loader.load());

        primaryStage.setTitle("2BSCS-2 Exam Seating Arrangement System");
        primaryStage.setScene(scene); 
        primaryStage.centerOnScreen();
        primaryStage.show();
    }

    public static void main(String[] args) {
        launch(args);
    }
}
